{
  "expo": {
    "name": "UdaciFitness",
    "description": "Triathlon tracker for the determined",
    "slug": "UdaciFitness",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/icon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.walkeknowudacifitness.exp",
      "supportsTablet": true
    },
    "android": {
      "package": "org.udacifitness.exp"
    }
  }
}